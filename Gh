import pandas as pd
import re

# Example DataFrame with multiple line breaks
df = pd.DataFrame({
    'combined': ['title: issue title\n\nDescription: issue description']
})

# Regex to handle multiple line breaks and case-insensitive match
df[['title', 'description']] = df['combined'].str.extract(
    r'title:\s*(.*?)\n+Description:\s*(.*)', 
    flags=re.IGNORECASE
)

print(df[['title', 'description']])
