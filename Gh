import matplotlib.pyplot as plt

# Sample data
column1 = [80, 75, 90, 60, 85]  # percentages from first column
column2 = [78, 74, 88, 65, 83]  # percentages from second column

# Create scatter plot
plt.figure(figsize=(8, 5))
plt.scatter(column1, column2, color='blue', edgecolor='black')

# Add labels and title
plt.xlabel('Column 1 (%)')
plt.ylabel('Column 2 (%)')
plt.title('Variation Between Two Percentage Columns')
plt.grid(True)
plt.axline((0, 0), slope=1, color='red', linestyle='--', label='y = x')  # optional reference line

# Show legend and plot
plt.legend()
plt.tight_layout()
plt.show()
