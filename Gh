import re

def extract_fields(text):
    match = re.search(r'issue title:\s*(.*?)\s*issue Description:\s*(.*)', text, re.DOTALL | re.IGNORECASE)
    if match:
        title = match.group(1).strip()
        description = match.group(2).strip()
        return pd.Series([title, description])
    else:
        return pd.Series([None, None])

df[['title', 'description']] = df['raw'].apply(extract_fields)
