from openpyxl import load_workbook
from collections import Counter

file_path = 'your_file.xlsx'  # Change to your file
wb = load_workbook(file_path)
ws = wb.active

column_index = 2  # Column B

color_counts = Counter()

for row in ws.iter_rows(min_col=column_index, max_col=column_index):
    for cell in row:
        fill = cell.fill
        fg = fill.fgColor
        if fg.type == 'rgb' and fg.rgb:
            color = fg.rgb[-6:].lower()
        elif fg.type == 'theme':
            color = f'theme_{fg.theme}'
        elif fg.type == 'indexed':
            color = f'indexed_{fg.indexed}'
        else:
            color = 'none'
        color_counts[color] += 1

print("Unique fill colors found in column:")
for color, count in color_counts.items():
    print(f"{color}: {count}")
