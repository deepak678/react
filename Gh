import pandas as pd

# Sample DataFrame
data = [
    ['Dict1', 'a', 'b'],
    ['Dict1', 'c', 'd'],
    ['Dict2', 'e', 'f'],
    ['Dict2', 'g', 'h'],
    ['Dict1', 'i', 'j']
]
df = pd.DataFrame(data)

# Group by the first column and create a list of dictionaries from the second and third columns
result_dict = df.groupby(df.iloc[:, 0]).apply(lambda x: x.apply(lambda y: {"key": y[1], "value": y[2]}, axis=1).tolist()).to_dict()

print(result_dict)
