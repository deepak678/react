import pandas as pd

# Example DataFrame
df = pd.DataFrame({
    'combined': ['title: issue title\nDescription: issue description']
})

# Use regex to extract title and description
df[['title', 'description']] = df['combined'].str.extract(r'title:\s*(.*?)\nDescription:\s*(.*)', flags=re.IGNORECASE)

print(df[['title', 'description']])
