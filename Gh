import pandas as pd

# Example DataFrame
df = pd.DataFrame({
    'title': ['Data Science Basics', 'AI and Machine Learning', 'Python Guide', 'Deep Learning Tips']
})

# Set of substrings to check
search_set = {'Python', 'AI', 'Data'}

# Filter rows where any element from the set is a substring in the 'title' column
mask = df['title'].apply(lambda x: any(term in x for term in search_set))

# New DataFrame with matching rows
filtered_df = df[mask]

print(filtered_df)
