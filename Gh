import pandas as pd
import re

# Example DataFrame
df = pd.DataFrame({
    'details': [
        '''"title": Fix login bug
"description": User is unable to log in on Safari.
Steps to reproduce:
1. Open browser
2. Go to login page
3. Submit credentials'''
    ]
})

# Function to extract title and description using multi-line regex
def extract_title_description(text):
    title_match = re.search(r'"title":(.*)\n"description":', text, re.DOTALL)
    description_match = re.search(r'"description":(.*)', text, re.DOTALL)
    
    title = title_match.group(1).strip() if title_match else None
    description = description_match.group(1).strip() if description_match else None
    return pd.Series([title, description])

df[['title', 'description']] = df['details'].apply(extract_title_description)
